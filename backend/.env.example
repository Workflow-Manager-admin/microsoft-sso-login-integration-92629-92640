# Azure App Registration variables
AZURE_AD_TENANT_ID=your-tenant-id-here
AZURE_AD_CLIENT_ID=your-app-client-id-here
AZURE_AD_CLIENT_SECRET=your-app-client-secret-here
AZURE_AD_REDIRECT_URI=http://localhost:3001/auth/microsoft/callback

# JWT secret key (choose a long random value)
JWT_SECRET=supersecretjwtkey

# Session secret key (choose a long random value)
SESSION_SECRET=session_secret_here

# FRONTEND base URL for login redirect
FRONTEND_URL=http://localhost:3000

# (Optional) PORT/host for Express server
PORT=3001
HOST=0.0.0.0
